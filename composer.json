{
    "name": "wyrihaximus/react-guzzle",
    "description": "Asyncronous Guzzle adapter powered by react/http-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "~4.0",
        "react/http-client": "^0.4.0",
        "react/dns": "^0.4.1",
        "wyrihaximus/react-guzzle-http-client": "^3.0.0",
        "guzzlehttp/psr7": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phake/phake": "~1.0.6",
        "squizlabs/php_codesniffer": "~1.5.0",
        "vectorface/dunit": "~2.0"
    },
    "suggest": {
        "guzzlehttp/oauth-subscriber": "0.1.* needed for oauth example"
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\React\\Guzzle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\React\\Tests\\Guzzle\\": "tests/"
        }
    }
}
